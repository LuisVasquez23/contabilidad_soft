delete from impuesto 
where NIT = '1111-111111-111-1';


select im.mes, im.sueldo_gravado, im.renta, im.bono, im.afp, im.isss, (em.sueldo + im.aguinaldo + im.vacaciones) from empleado em
join impuesto im on em.NIT = im.NIT
WHERE em.NIT = '1111-111111-111-1';

select * from empleado;

delete from impuesto

select em.nombre, em.NIT,  sum(em.sueldo)as sueldo, sum(im.bono)as comision, (sum(em.sueldo) + sum(im.bono)+sum(im.vacaciones)+sum(im.aguinaldo)) as ingreso, 
sum(im.sueldo_gravado)as gravado, sum(im.afp)as afp, sum(im.isss)as isss, sum(im.renta) as isr, 
round((sum(im.afp) + sum(im.isss) + sum(im.renta)),3) as descuento, round(((sum(em.sueldo) + sum(im.bono)+ sum(im.vacaciones)+sum(im.aguinaldo)) - (sum(im.afp) + sum(im.isss) + sum(im.renta))), 3) as pagado
from empleado em
join impuesto im on em.NIT = im.NIT
where im.NIT = '1111-111111-111-1';


select em.NIT, em.nombre, em.fecha_contrato, em.cargo, em.sueldo, im.bono, (em.sueldo + im.bono)as ingreso, (em.sueldo*0.0775) as AFP, (em.sueldo * 0.075) as isss, (1000*.1)as insaford,
((em.sueldo*0.0775)+(em.sueldo * 0.075) + (1000*.1))as total  from empleado em
join impuesto im on em.NIT = im.NIT
where em.NIT = '1111-111111-111-1'
LIMIT 1;
